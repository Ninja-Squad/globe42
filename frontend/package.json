{
  "name": "globe42",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --no-progress",
    "test": "ng test --code-coverage --no-progress --no-watch",
    "bundlesize": "bundlesize --enable-github-checks",
    "codecov": "codecov --root=..",
    "lint": "ng lint > build/eslint-result.txt",
    "format": "prettier --write \"src/**/*.+(ts|js|json|html|scss)\"",
    "format:check": "mkdirp build && prettier --check \"src/**/*.+(ts|js|json|html|scss)\" > build/prettier-result.txt 2>&1",
    "ngcc": "ngcc --properties es2015 browser module main --async false"
  },
  "private": true,
  "engines": {
    "node": ">=14.17",
    "yarn": ">=1.22.10 <2.0.0"
  },
  "dependencies": {
    "@angular/common": "13.0.0",
    "@angular/compiler": "13.0.0",
    "@angular/core": "13.0.0",
    "@angular/forms": "13.0.0",
    "@angular/platform-browser": "13.0.0",
    "@angular/platform-browser-dynamic": "13.0.0",
    "@angular/router": "13.0.0",
    "@ng-bootstrap/ng-bootstrap": "11.0.0-beta.2",
    "bootstrap": "5.1.3",
    "chart.js": "3.6.0",
    "font-awesome": "4.7.0",
    "luxon": "2.0.2",
    "ngx-valdemort": "7.0.0",
    "rxjs": "7.4.0",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.0.3",
    "@angular-eslint/builder": "12.6.1",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/cli": "13.0.3",
    "@angular/compiler-cli": "13.0.0",
    "@angular/localize": "13.0.0",
    "@types/jasmine": "3.10.2",
    "@types/luxon": "2.0.5",
    "@types/node": "14.17.32",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "bundlesize2": "0.0.31",
    "codecov": "3.8.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jasmine-core": "3.10.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.7",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "ngx-speculoos": "7.0.0",
    "prettier": "2.4.1",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  },
  "bundlesize": [
    {
      "path": "./dist/main.*.js",
      "maxSize": "340Kb"
    }
  ]
}