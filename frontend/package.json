{
  "name": "globe42",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --no-progress",
    "test": "ng test --code-coverage --no-progress --no-watch",
    "bundlesize": "bundlesize --enable-github-checks",
    "codecov": "codecov --root=..",
    "lint": "ng lint > build/eslint-result.txt",
    "format": "prettier --write \"src/**/*.+(ts|js|json|html|scss)\"",
    "format:check": "mkdirp build && prettier --check \"src/**/*.+(ts|js|json|html|scss)\" > build/prettier-result.txt 2>&1"
  },
  "private": true,
  "engines": {
    "node": ">=14.17",
    "yarn": ">=1.22.10 <2.0.0"
  },
  "dependencies": {
    "@angular/common": "14.0.0",
    "@angular/compiler": "14.0.0",
    "@angular/core": "14.0.0",
    "@angular/forms": "14.0.0",
    "@angular/platform-browser": "14.0.0",
    "@angular/platform-browser-dynamic": "14.0.0",
    "@angular/router": "14.0.0",
    "@ng-bootstrap/ng-bootstrap": "12.1.2",
    "@popperjs/core": "2.11.5",
    "bootstrap": "5.1.3",
    "chart.js": "3.8.0",
    "font-awesome": "4.7.0",
    "luxon": "2.4.0",
    "ngx-valdemort": "8.0.0",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.1",
    "@angular-eslint/builder": "13.3.0",
    "@angular-eslint/eslint-plugin": "13.3.0",
    "@angular-eslint/eslint-plugin-template": "13.3.0",
    "@angular-eslint/template-parser": "13.3.0",
    "@angular/cli": "14.0.1",
    "@angular/compiler-cli": "14.0.0",
    "@angular/localize": "14.0.0",
    "@types/jasmine": "4.0.3",
    "@types/luxon": "2.3.2",
    "@types/node": "14.18.20",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "bundlesize2": "0.0.31",
    "codecov": "3.8.3",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jasmine-core": "4.2.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.20",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.1",
    "karma-jasmine-html-reporter": "2.0.0",
    "ngx-speculoos": "8.0.0",
    "prettier": "2.6.2",
    "ts-node": "10.8.1",
    "typescript": "4.6.4"
  },
  "bundlesize": [
    {
      "path": "./dist/main.*.js",
      "maxSize": "340Kb"
    }
  ]
}
