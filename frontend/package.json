{
  "name": "globe42",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --no-progress",
    "test": "ng test --code-coverage --no-progress --no-watch",
    "bundlesize": "bundlesize --enable-github-checks",
    "codecov": "codecov --root=..",
    "lint": "ng lint > build/eslint-result.txt",
    "format": "prettier --write \"src/**/*.+(ts|js|json|html|scss)\"",
    "format:check": "mkdirp build && prettier --check \"src/**/*.+(ts|js|json|html|scss)\" > build/prettier-result.txt 2>&1",
    "ngcc": "ngcc --properties es2015 browser module main --async false"
  },
  "private": true,
  "engines": {
    "node": ">=14.17",
    "yarn": ">=1.22.10 <2.0.0"
  },
  "dependencies": {
    "@angular/common": "12.2.1",
    "@angular/compiler": "12.2.1",
    "@angular/core": "12.2.1",
    "@angular/forms": "12.2.1",
    "@angular/platform-browser": "12.2.1",
    "@angular/platform-browser-dynamic": "12.2.1",
    "@angular/router": "12.2.1",
    "@ng-bootstrap/ng-bootstrap": "11.0.0-beta.2",
    "bootstrap": "5.1.0",
    "chart.js": "3.5.0",
    "font-awesome": "4.7.0",
    "luxon": "2.0.2",
    "ngx-valdemort": "6.0.0",
    "rxjs": "7.3.0",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.1.2",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "12.1.2",
    "@angular/compiler-cli": "12.2.1",
    "@angular/localize": "12.2.1",
    "@types/jasmine": "3.8.1",
    "@types/luxon": "2.0.0",
    "@types/node": "14.17.5",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "bundlesize2": "0.0.30",
    "codecov": "3.8.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "ngx-speculoos": "6.0.0",
    "prettier": "2.3.2",
    "ts-node": "10.2.0",
    "typescript": "4.3.5"
  },
  "bundlesize": [
    {
      "path": "./dist/main.*.js",
      "maxSize": "340Kb"
    }
  ]
}
